{
  "name": "react-landing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "data.maybe": "^1.2.3",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-hot-loader": "^4.3.12",
    "react-intl": "^2.7.2",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.1.0",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-persist": "4.8.3",
    "reselect": "^4.0.0",
    "styled-components": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "eslint": "node_modules/.bin/eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js, jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "concurrent": false,
    "ignore": [
      "*.json",
      "**/package-lock.json"
    ]
  }
}
